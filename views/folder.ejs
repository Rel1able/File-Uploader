<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <a href="/">Home</a>
    <h1><%=folder.foldername%></h1>
    <ul>
        <% files.forEach((file) => {%>
            <li><%=file.filename%></li>
        <%})%>
    </ul>
    <form action="/folders/<%=folder.id%>/upload-file" method="POST" enctype="multipart/form-data">
        <input type="file" name="file" required>
        <button type="submit">Add</button>
    </form>
</body>
</html>